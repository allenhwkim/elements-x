{
  "name": "elements-x",
  "version": "3.3.6",
  "description": "collection of custom elements",
  "files": [
    "dist/lib"
  ],
  "main": "./dist/lib/index.umd.js",
  "module": "./dist/lib/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/lib/index.umd.js",
      "require": "./dist/lib/index.mjs"
    }
  },
  "scripts": {
    "start": "rm -rf node_modules/.cache/storybook && storybook dev -p 6006",
    "build": "npm run build:sb && npm run build:lib && npm run build:lib:core && npm run build:lib:extended",
    "build:lib": "npm run build:lib:index && npm run build:lib:core && npm run build:lib:extended && npm run build:lib:copy",
    "build:lib:index": "DIR=index vite build --outDir dist/lib",
    "build:lib:core": "DIR=core vite build --outDir dist/lib",
    "build:lib:extended": "DIR=extended vite build --outDir dist/lib",
    "build:lib:copy": "cp lib/test.html dist/lib",
    "build:sb": "storybook build -o dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@storybook/addon-a11y": "^7.6.4",
    "@storybook/addon-actions": "^7.6.4",
    "@storybook/addon-docs": "^7.6.4",
    "@storybook/addon-essentials": "^7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/addon-links": "^7.6.4",
    "@storybook/addon-mdx-gfm": "^7.6.4",
    "@storybook/addons": "^7.6.4",
    "@storybook/blocks": "^7.6.4",
    "@storybook/html": "^7.6.4",
    "@storybook/html-vite": "^7.6.4",
    "@storybook/jest": "0.2.3",
    "@storybook/testing-library": "0.2.2",
    "@storybook/theming": "^7.6.4",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/mdx": "^2.0.10",
    "@types/morphdom": "^2.4.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/ungap__structured-clone": "^0.3.3",
    "@ungap/structured-clone": "^1.0.2",
    "babel-loader": "^9.1.3",
    "body-scroll-lock": "^4.0.0-beta.0",
    "bootstrap": "^5.3.2",
    "css-loader": "^6.7.3",
    "grapesjs": "^0.21.7",
    "html-to-image": "^1.11.11",
    "morphdom": "^2.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.10.1",
    "sass": "^1.69.5",
    "sass-loader": "^13.2.0",
    "storybook": "^7.6.4",
    "style-loader": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.7"
  },
  "license": "MIT"
}
